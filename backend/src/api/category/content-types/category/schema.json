{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "logo_name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "logo_url": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "color": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "order": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "companies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::company.company",
      "inversedBy": "categories"
    },
    "coupons": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::coupon.coupon",
      "inversedBy": "categories"
    }
  }
}
